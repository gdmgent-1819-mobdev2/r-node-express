{
  "name": "r-node-express",
  "version": "1.0.0",
  "description": "React Node Express Example Project",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "server-start": "nodemon src/server/index.js",
    "client-start": "cross-env cd src\\client && react-scripts start",
    "client-build": "cross-env cd src\\client && react-scripts build",
    "client-test": "cross-env cd src\\client && react-scripts test",
    "dev-start": "concurrently \"npm run server-start\" \"npm run client-start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdmgent-1819-mobdev2/r-node-express.git"
  },
  "keywords": [
    "react",
    "node",
    "express",
    "momgodb",
    "api",
    "redux",
    "sass",
    "react-native",
    "expo",
    "jest",
    "nmd"
  ],
  "author": "Philippe De Pauw - Waterschoot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gdmgent-1819-mobdev2/r-node-express/issues"
  },
  "homepage": "https://github.com/gdmgent-1819-mobdev2/r-node-express#readme",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.10"
  },
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8"
  }
}
